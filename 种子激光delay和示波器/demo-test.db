# ein Multi-Bit-Binary Record bzw. ein Bitfeld (bis zu 16 Bit)
# wird mit Integer beschrieben und kann als Einzelbits im Dm2k
# dargestellt werden
record(mbbi,"caLab:mbbi") {	
    field(DESC,"mbbi")
    field(ZRST,"0")
    field(ONST,"bit 0")
    field(TWST,"bit 1")
    field(THST,"bit 0+1")
    field(FRST,"bit 2")
    field(FVST,"bit 2+0")
    field(SXST,"bit 2+1")
    field(SVST,"bit 2+1+0")
    field(ZRSV,"NO_ALARM")
    field(ONSV,"NO_ALARM")
    field(TWSV,"NO_ALARM")
    field(THSV,"MAJOR")
    field(FRSV,"NO_ALARM")
    field(FVSV,"MAJOR")
    field(SXSV,"MAJOR")
    field(SVSV,"MAJOR")
    field (PINI, "YES")
}